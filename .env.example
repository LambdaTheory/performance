# Supabase 配置
REACT_APP_SUPABASE_URL=your_supabase_project_url
REACT_APP_SUPABASE_ANON_KEY=your_supabase_anon_key

# 应用配置
REACT_APP_APP_NAME=绩效问卷管理系统
REACT_APP_VERSION=1.0.0

# 飞书OAuth配置
REACT_APP_FEISHU_APP_ID=your_feishu_app_id

# 后端API配置
REACT_APP_API_BASE_URL=http://localhost:3001/api
REACT_APP_API_SECRET_KEY=your_api_secret_key

# 管理员邮箱列表（用逗号分隔）- 已弃用，飞书API不返回邮箱
REACT_APP_ADMIN_EMAILS=admin1@example.com,admin2@example.com

# 管理员用户ID列表（用逗号分隔）
REACT_APP_ADMIN_IDS=user_id_1,user_id_2

# 管理员OpenID列表（用逗号分隔）
REACT_APP_ADMIN_OPEN_IDS=ou_xxxxxxxxxxxxxxxxxxxx,ou_yyyyyyyyyyyyyyyyyyyy

# 开发配置
PORT=3000
GENERATE_SOURCEMAP=false

# Docker 部署配置
# 后端环境变量
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
FEISHU_APP_ID=your_feishu_app_id
FEISHU_APP_SECRET=your_feishu_app_secret
FEISHU_TENANT_ACCESS_TOKEN=your_feishu_tenant_access_token
JWT_SECRET=your_jwt_secret_key
NODE_ENV=production

# 前端环境变量（Docker 构建时需要）
# 注意：前端变量需要同时设置 REACT_APP_ 前缀的版本
REACT_APP_SUPABASE_URL=your_supabase_project_url
REACT_APP_SUPABASE_ANON_KEY=your_supabase_anon_key
REACT_APP_FEISHU_APP_ID=your_feishu_app_id
REACT_APP_FEISHU_REDIRECT_URI=http://your-domain.com
REACT_APP_API_BASE_URL=http://your-domain.com/api
REACT_APP_API_SECRET_KEY=your_api_secret_key
REACT_APP_ADMIN_IDS=user_id_1,user_id_2
REACT_APP_ADMIN_OPEN_IDS=ou_xxxxxxxxxxxxxxxxxxxx,ou_yyyyyyyyyyyyyyyyyyyy
REACT_APP_APP_NAME=绩效问卷管理系统
REACT_APP_VERSION=1.0.0